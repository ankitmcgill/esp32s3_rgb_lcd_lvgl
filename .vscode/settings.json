{
    "git.openRepositoryInParentFolders": "never",
    "workbench.tree.indent": 16,
    "editor.experimental.treeSitterTelemetry": true,
    "files.associations": {
        "*.h": "c",
        "*.backup": "c"
    },
    "C_Cpp.default.compilerPath": "/usr/bin/clang",
    "C_Cpp.files.exclude": {
        "**/.vscode": true,
        "**/.vs": true,
        "**/managed_components": true,
        "**/project_ui": true,
    },
}